modules = ["python-3.11", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["glibcLocales"]

[agent]
expertMode = true
integrations = ["google-sheet:1.0.0", "google-drive:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dashboard-server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "orders-cleanup"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "unified-shipstation-sync"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "shipstation-upload"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "xml-import"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "duplicate-scanner"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "lot-mismatch-scanner"

[[workflows.workflow]]
name = "dashboard-server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "orders-cleanup"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python src/scheduled_cleanup.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "unified-shipstation-sync"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python src/unified_shipstation_sync.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "shipstation-upload"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python src/scheduled_shipstation_upload.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "xml-import"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python src/scheduled_xml_import.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "duplicate-scanner"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python src/scheduled_duplicate_scanner.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "lot-mismatch-scanner"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python src/scheduled_lot_mismatch_scanner.py"

[workflows.workflow.metadata]
outputType = "console"

[deployment]
deploymentTarget = "vm"
run = ["bash", "start_all.sh"]

